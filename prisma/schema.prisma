generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Job {
  id        String    @id @default(cuid())
  title     String
  company   String?
  location  String?
  url       String?
  status    JobStatus
  notes     String?
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
}

enum JobStatus {
  draft
  applied
  interview
  offer
  rejected
  archived
}
